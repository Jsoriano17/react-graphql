{
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "outDir": "../thelitas-mobile/node_modules/@tesseractcollective/react-native-graphql/dist/src",
    "jsx": "react",
    "paths": {
      "hooks": [
        "hooks"
      ],
      "support": [
        "support"
      ],
      "components": [
        "components"
      ],
      "types": [
        "types"
      ],
    },
  },
  "include": [
    "src/index.native.ts",
    "src/components/shared/*.{ts|tsx|js|jsx}",
    "src/components/native/*.{ts|tsx|js|jsx}",
    "src/hooks/*.{ts|tsx|js|jsx}",
    "src/support/*.{ts|tsx|js|jsx}",
    "src/types/*.{ts|tsx|js|jsx}"
  ],
}
